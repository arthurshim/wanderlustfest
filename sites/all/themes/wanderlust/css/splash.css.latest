@-webkit-keyframes flagish
{
0%              {-webkit-transform: rotate(0deg);}
20%             {-webkit-transform: rotate(2deg);}
60%             {-webkit-transform: rotate(7deg);}
80%             {-webkit-transform: rotate(-7deg);}
100%    {-webkit-transform: rotate(0deg);}


}
@-moz-keyframes flagish
{
0%              {-webkit-transform: rotate(0deg);}
20%             {-webkit-transform: rotate(2deg);}
60%             {-webkit-transform: rotate(7deg);}
80%             {-webkit-transform: rotate(-7deg);}
100%    {-webkit-transform: rotate(0deg);}

}


@-moz-keyframes flagish
{
0%              {-ms-transform: rotate(0deg);}
20%             {-ms-transform: rotate(2deg);}
60%             {-ms-transform: rotate(7deg);}
80%             {-ms-transform: rotate(-7deg);}
100%    {-ms-transform: rotate(0deg);}

}


.yoga .icon:hover {

    -webkit-animation: bob 1s ;
        -moz-animation: bob 1s;
        -ms-animation: bob 1s;
}



.icon.studio, .studio .icon {
  background: url(../images/studio.png) no-repeat;
  height: 33px;
  width: 24px;
  top: 5px;
  -webkit-transition-timing-function: ease-out;
  -webkit-transition-duration: 500ms;
  
  -0-transition-timing-function: ease-out;
  -0-transition-duration: 500ms;
  
  -ms-transition-timing-function: ease-out;
  -ms-transition-duration: 500ms;
  
  -moz-transition-timing-function: ease-out;
  -moz-transition-duration: 500ms;
}
.studio .icon:hover {
    background: url(../images/studio_hover.png) no-repeat;

}


