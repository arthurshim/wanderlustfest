  overflow: hidden;
}

#wanderdata{
  position:absolute;
  display:none;
}

 #container {
  height: 100%;
  /*width: 1350px;
  height : 100%;*/
  width : 100%;
  overflow: hidden;
}

#map-1-container {
  
  overflow: hidden !important;
  position: relative;
  width : 1560px;
  height:910px;
}

.map-viewport {
  border: 1px solid black;
  margin: 0 0 20px;
  overflow: hidden;
  position: relative;
  height:910px;
  width : 1560px;
}

#map-1 {
  cursor: move;
  width: 2146px;
  z-index: 20;
  height: 1170px;
  position: absolute;
  left: -275px;/*map image make full height and width*/
}
  
.mapz-constraint{
  z-index: 5;
}





